              "example": "New York"
            },
            "zip": {
              "type": "string",
              "example": "10001"
            },
            "country": {
              "type": "string",
              "example": "USA"
            }
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "readOnly": true,
          "description": "Timestamp when the student account was created"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time",
